{
  "version": 2,
  "builds": [
    {
      "src": "trydjango/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "trydjango/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "trydjango.settings"
  },
  "functions": {
    "trydjango/wsgi.py": {
      "includeFiles": "**"
    }
  }
}