{
  "version": 2,
  "outputDirectory": "static/",
  "buildCommand": "python3 -m django collectstatic --noinput --clear --settings=trydjango.settings",
  "functions": {
    "api/wsgi.py": {
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "trydjango.settings"
  }
}