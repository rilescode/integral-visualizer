{
  "version": 2,
  "builds": [
    {
      "src": "trydjango/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.11"
      }
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "trydjango/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "trydjango.settings"
  }
}